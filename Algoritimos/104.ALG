Algoritmo "semnome"

Var
  idade: inteiro
  nome, sexo : caractere
  


Inicio
  escreval("digite o nome: ")
  leia(nome)
  escreval("digite a idade: ")
  leia(idade)
  escreval("digite seu sexo: ")
  leia(sexo)
  se (sexo = "f")  e idade < 25  entao
  escreval(nome, "aceita:")
  senao
  escreval(nome, "não aceita:")
  fimse



Fimalgoritmo
